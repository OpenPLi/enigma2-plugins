msgstr "'dvbdate' не установлен!"

msgid "'ntpdate' is not installed!"
msgstr "'ntpdate' не установлен!"

msgid "1 hour"
msgstr "1 час"

msgid "12 hours"
msgstr "12 часов"

msgid "15 mins"
msgstr "15 минут"

msgid "2 hours"
msgstr "2 часа"

msgid "24 hours"
msgstr "24 часа"

