msgid "'dvbdate' is not installed!"
msgstr "'dvbdate' neįdiegtas!"

msgid "'ntpdate' is not installed!"
msgstr "'NtpDate' neįdiegtas!"

msgid "1 hour"
msgstr "1 val."

msgid "12 hours"
msgstr "12 val."

